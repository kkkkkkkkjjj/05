#include <stdio.h>
#include <stdlib.h>

int main() {
    
    int a,b;
    char operator;

    printf("enter the calculation :\n");
    scanf("%d %c %d ", &a, &operator, &b);

    int result;

    if (operator == '+'){
        result = a + b;
    } else if (operator == '-'){
        result = a - b;
    } else if (operator == '*'){
        result = a * b;
    } 

    printf( " = %d ",result);

    return 0;
        
    }


