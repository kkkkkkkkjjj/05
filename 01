#include <stdio.h>
#include <stdlib.h>

int main() {
    
    int answer = 60;
    int guess ;
    int try = 0;

    do {
        printf( "guess a number :\n");
        scanf("%d", &guess);
        try++;

        if(guess > answer){
            printf(" high !\n ");
        } else if (guess < answer){
            printf("low !\n");
        } else {
            printf( "Congratulations! trials : %d\n",try);
        }

    } while (guess != answer);
    

    return 0;
        
    }


